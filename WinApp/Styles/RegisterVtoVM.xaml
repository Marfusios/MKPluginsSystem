<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    
                    xmlns:localV="clr-namespace:WinApp.Views" 
                    xmlns:localVM="clr-namespace:WinApp.ViewModels" 
                    >

    <DataTemplate DataType="{x:Type localVM:MainPanelViewModel}">
        <localV:MainPanel />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type localVM:PluginPanelVM}">
        <localV:PluginPanel />
    </DataTemplate>


</ResourceDictionary>
